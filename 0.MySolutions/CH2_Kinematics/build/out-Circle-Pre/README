# Circle1.out
# Commands to run in gnuplot 
file="Circle1.out"; t0=0; tf=10; dt=0.01; set xrange [-2:2]; set yrange [-2:2]
load "animate2D.gnu"


plot "Circle1.out" using 1:2 with lines title "x(t)"; replot "Circle1.out" using 1:3 with lines title "y(t)"

plot "Circle1.out" using 1:4 with lines title "vx(t)"; replot "Circle1.out" using 1:5 with lines title "vy(t)"

plot "Circle1.out" using 1:6 with lines title "ax(t)"; replot "Circle1.out" using 1:7 with lines title "ay(t)"

plot "Circle1.out" using 1:8 with lines title "theta(t)"
